<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Primary Color -->
    <Color x:Key="PrimaryColor">#444054</Color>
    <Color x:Key="PrimaryColorL10">#4B465C</Color>
    <Color x:Key="PrimaryColorL20">#524C64</Color>
    <Color x:Key="PrimaryColorL30">#58536D</Color>
    <Color x:Key="PrimaryColorL40">#5F5A75</Color>
    <Color x:Key="PrimaryColorD10">#3D394B</Color>
    <Color x:Key="PrimaryColorD20">#363343</Color>
    <Color x:Key="PrimaryColorD30">#2F2D3A</Color>
    <Color x:Key="PrimaryColorD40">#292632</Color>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />

    <!-- Secondary Color -->
    <Color x:Key="SecondaryColor">#378495</Color>
    <Color x:Key="SecondaryColorL10">#3D91A4</Color>
    <Color x:Key="SecondaryColorL20">#429EB3</Color>
    <Color x:Key="SecondaryColorL30">#48ABCA</Color>
    <Color x:Key="SecondaryColorL40">#4DB8D0</Color>
    <Color x:Key="SecondaryColorD10">#327782</Color>
    <Color x:Key="SecondaryColorD20">#2D6A77</Color>
    <Color x:Key="SecondaryColorD30">#285D68</Color>
    <Color x:Key="SecondaryColorD40">#235059</Color>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
    <Color x:Key="SecondaryTransparentBackground">#80378495</Color>
    <SolidColorBrush x:Key="SecondaryTransparentBackgroundBrush" Color="{StaticResource SecondaryTransparentBackground}" />

    <!-- Accent Color -->
    <Color x:Key="AccentColor">#ED6A5A</Color>
    <Color x:Key="AccentColorL10">#ED7563</Color>
    <Color x:Key="AccentColorL20">#ED804C</Color>
    <Color x:Key="AccentColorL30">#ED8B42</Color>
    <Color x:Key="AccentColorL40">#ED9641</Color>
    <Color x:Key="AccentColorD10">#D55F51</Color>
    <Color x:Key="AccentColorD20">#BE5548</Color>
    <Color x:Key="AccentColorD30">#A84C3F</Color>
    <Color x:Key="AccentColorD40">#924336</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <Color x:Key="RedTransparentBackground">#80924336</Color>
    <SolidColorBrush x:Key="RedTransparentBackgroundBrush" Color="{StaticResource RedTransparentBackground}" />

    <!-- Light Accent Color -->
    <Color x:Key="LightAccentColor">#E0E1DD</Color>
    <Color x:Key="LightAccentColorL10">#F6F8F3</Color>
    <Color x:Key="LightAccentColorL20">#D6D9D4</Color>
    <Color x:Key="LightAccentColorL30">#D6D9D4</Color>
    <Color x:Key="LightAccentColorL40">#D6D9D4</Color>
    <Color x:Key="LightAccentColorD10">#CACBC7</Color>
    <Color x:Key="LightAccentColorD20">#B4B5B2</Color>
    <Color x:Key="LightAccentColorD30">#9E9F9D</Color>
    <Color x:Key="LightAccentColorD40">#898A89</Color>
    <SolidColorBrush x:Key="LightAccentBrush" Color="{StaticResource LightAccentColor}" />

    <Color x:Key="DarkTransparentBackground">#80292632</Color>
    <SolidColorBrush x:Key="DarkTransparentBackgroundBrush" Color="{StaticResource DarkTransparentBackground}" />
    <Color x:Key="LightTransparentBackground">#20E0E1DD</Color>
    <SolidColorBrush x:Key="LightTransparentBackgroundBrush" Color="{StaticResource LightTransparentBackground}" />

    <!-- Radial Gradient Background using Primary and Secondary Colors -->
    <RadialGradientBrush x:Key="BackgroundGradient" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5">
        <GradientStop Color="{StaticResource PrimaryColor}" Offset="0.0"/>
        <GradientStop Color="{StaticResource PrimaryColorD20}" Offset="1.0"/>
    </RadialGradientBrush>
</ResourceDictionary>